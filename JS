    }
    for(var j=m+1;j<=5;j++)
    {
        var id="s"+j;
        document.getElementById(id).style.color="#555555";
        document.getElementById(id).style.textShadow="0 2px 2px rgba(255,255,255,0.13)";
    }
    
    
    
}
