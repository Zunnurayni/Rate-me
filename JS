var c;
function st(m)
{
    if(m==1)
    {
        document.getElementById("r").innerHTML="ğŸ˜¢";
    }
    else if(m==2)
    {
        document.getElementById("r").innerHTML="â˜¹";
    }
    else if(m==3)
    {
        document.getElementById("r").innerHTML="ğŸ˜";
    }
    else if(m==4)
    {
        document.getElementById("r").innerHTML="ğŸ™‚";
    }
    else if(m==5)
    {
        document.getElementById("r").innerHTML="ğŸ˜Š";
    }
    for(var i=1;i<=m;i++)
    {
        var id="s"+i;
        document.getElementById(id).style.color="yellow";
